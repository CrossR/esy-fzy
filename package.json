{
  "name": "esy-fzy",
  "version": "5.9.1000",
  "description": "esy-enabled build for fzy",
  "license": "MIT",
  "esy": {
    "build": [
        ["make"],
        ["ar", "rcs", "libfzy.a", "src/match.o", "src/choices.o", "src/options.o"]
    ],
    "buildsInSource": true
  },
  "exportedEnv": {
        "FZY_INCLUDE_PATH": {
            "val": "#{self.install / 'src'}",
            "scope": "global"
        },
        "FZY_LIB_PATH": {
            "val": "#{self.lib}",
            "scope": "global"
        },
        "LD_LIBRARY_PATH": {
            "val": "#{self.lib : $LD_LIBRARY_PATH}",
            "scope": "global"
        }
    },
  "dependencies": { }
}
